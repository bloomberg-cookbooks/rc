# This file is managed by Chef; all manual changes will be lost!
<%- @new_resource.options.map do |k, v| %>
<%- if v.kind_of?(Array) %>
<%= [k, "(#{v.map(&:to_s).join(' ')})"].join('=') %>
<%- elsif v.kind_of?(Hash) %>
<%= v.map { |k2, v2| [[k, k2].join('_'), "\"#{v2}\""].join('=') }.join("\n") %>
<%- else %>
<%= [k, "\"#{v}\""].join('=') %>
<%- end %>
<%- end.map { |v| "export #{v}" }.join("\n") %>
